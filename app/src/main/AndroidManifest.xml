                android:value="bitw.techexpo.ProfileActivity" />
        </activity>
        <activity
            android:name=".activity_studysit"
            android:label=""
            android:parentActivityName=".ProfileActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bitw.techexpo.ProfileActivity" />
        </activity>
        <activity
            android:name=".activity_planner"
            android:label=""
            android:parentActivityName=".ProfileActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bitw.techexpo.ProfileActivity" />
        </activity>
        <activity
            android:name=".AddNoteActivity"
            android:label=""
            android:parentActivityName=".activity_planner"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bitw.techexpo.activity_planner" />
        </activity>
        <activity
            android:name=".NotePreviewActivity"
            android:label=""
            android:parentActivityName=".activity_planner"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bitw.techexpo.activity_planner" />
        </activity>
        <activity android:name=".activity_aboutus"
            android:label=""
            android:parentActivityName=".ProfileActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="bitw.techexpo.ProfileActivity" />

        </activity>
    </application>

</manifest>